{
    "esm": {
        "version": "2.5",
        "website": "http://www.ezservermonitor.com",
        "check_updates": false,
        "auto_refresh": 10,
        "theme": "blue",
        "custom_title": "Server status"
    },
    "cpu": {
        "enable_temperature": false
    },
    "disk": {
        "show_tmpfs": false,
        "show_filesystem": true,
        "ignore_mounts": []
    },
    "ping": {
        "hosts": []
    },
    "last_login": {
        "enable": false,
        "max": 5
    },
    "services": {
        "show_port": true,
        "list": []
    }
}
